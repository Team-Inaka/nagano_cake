<p id="notice"><%= notice %></p>

<h1>Costomers Cart Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @costomers_cart_items.each do |costomers_cart_item| %>
      <tr>
        <td><%= link_to 'Show', costomers_cart_item %></td>
        <td><%= link_to 'Edit', edit_costomers_cart_item_path(costomers_cart_item) %></td>
        <td><%= link_to 'Destroy', costomers_cart_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Costomers Cart Item', new_costomers_cart_item_path %>
